# Immich Environment Variables
# Copy to .env and adjust for your deployment

# TreeOS required variables
COMPOSE_PROJECT_NAME=ontree-immich
COMPOSE_SEPARATOR=-

# Ports
IMMICH_PORT=2283

# Database
DB_USERNAME=immich
DB_PASSWORD=immich_db_password_change_me
DB_DATABASE=immich

# Auth
JWT_SECRET=change-me-secret

# Logging
LOG_LEVEL=log

# Timezone
TZ=UTC
