# TreeOS Configuration
COMPOSE_PROJECT_NAME=ontree-unifi-network
COMPOSE_SEPARATOR=-

# MongoDB Configuration
MONGO_DBNAME=unifi

# Application Configuration
PUID=1000
PGID=1000
TZ=UTC

# Port Configuration
UNIFI_HTTPS_PORT=8443
UNIFI_HTTP_PORT=8080
UNIFI_STUN_PORT=3478
UNIFI_DISCOVERY_PORT=10001
UNIFI_L2_DISCOVERY_PORT=1900
UNIFI_SPEEDTEST_PORT=6789
UNIFI_SYSLOG_PORT=5514

# Note: When deploying, copy this file to .env and update MONGO_PASS with a secure password