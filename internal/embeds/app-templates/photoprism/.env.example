# PhotoPrism Environment Variables
# Copy to .env and adjust for your deployment

# TreeOS required variables
COMPOSE_PROJECT_NAME=ontree-photoprism
COMPOSE_SEPARATOR=-

# App ports and URLs
PHOTOPRISM_PORT=2342
PHOTOPRISM_SITE_URL=http://localhost:2342/

# Admin credentials
PHOTOPRISM_ADMIN_PASSWORD=change-me

# Database settings
PHOTOPRISM_DATABASE=photoprism
PHOTOPRISM_DB_USER=photoprism
PHOTOPRISM_DB_PASSWORD=photoprism_db_password_change_me
PHOTOPRISM_DB_ROOT_PASSWORD=photoprism_root_password_change_me

# App behavior
PHOTOPRISM_READONLY=false

# Timezone
TZ=UTC
