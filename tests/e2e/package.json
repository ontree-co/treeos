{
  "name": "ontree-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for OnTree Node application",
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "HEADED=true playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:vscode": "playwright test --config=playwright.config.vscode.js",
    "test:setup": "playwright test auth/setup.test.js",
    "test:auth": "playwright test auth/",
    "test:dashboard": "playwright test dashboard/",
    "test:apps": "playwright test apps/",
    "test:docker": "playwright test docker/",
    "test:all": "playwright test"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2"
  }
}
